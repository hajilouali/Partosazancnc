
@{
    ViewBag.Title = "پیغام های ارسالی";
}


<h2>@ViewBag.Title</h2>

<button onclick="getValuePublish()" type="button" class="btn btn-success btn-sm"> خوانده شدن انتخابی ها</button>
<button onclick="getValueDraft()" type="button" class="btn btn-info btn-sm">خوانده نشدن انتخابی ها</button>
<button onclick="getValueDelete()" type="button" class="btn btn-danger btn-sm">حذف انتخابی ها</button>

<div class="margin-top-20" id="Comment_content">

</div>

<div id="myModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">

            <!-- Modal Header -->
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="myModalLabel">ویرایش نظر کاربران</h4>
            </div>

            <!-- Modal Body -->
            <div class="modal-body" id="modalbody">

            </div>

            <!-- Modal Footer -->
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">بستن</button>
            </div>

        </div>
    </div>
</div>

@section Scripts
{
    <script src="~/ckeditor/ckeditor.js"></script>

    <script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
    <script src="~/Areas/Admin/assets/js/Contactus.js"></script>
}


